html 
 body
  h Orders
  form(method="post" action="/orders") 
   table(border= '1px solid black') 
    tr
     th Edit/Delete
     th ID
     th Order Number
     th Part Number
     th Priority 
     th Status
     th Comments
    each val in orderNum
     tr
      td
       input(type= "checkbox" name= "checked" value=val.id)
      td= val.id 
      td= val.ordernumber
      td= val.partnumber
      td= val.priority
      td= val.status
      td= val.comments
   input(type="submit")
  form(method="get" action="/login")
   input(type="submit" value="Go Back")
